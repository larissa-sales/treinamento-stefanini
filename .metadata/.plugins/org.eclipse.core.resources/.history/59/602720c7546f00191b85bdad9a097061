package com.stefanini.projeto.model;

import java.io.Serializable;

import javax.persistence.Column;
import javax.persistence.Embeddable;

@Embeddable
public class PraiaCorPK implements Serializable{

	private static final long serialVersionUID = 1L;

	@Column(name = "CA_NU")
	private Long idPraia;

	@Column(name = "CO_NU")
	private Long idCor;

	public PraiaCorPK() {
		super();
	}

	public PraiaCorPK(Long idPraia, Long idCor) {
		super();
		this.idPraia = idPraia;
		this.idCor = idCor;
	}

	public Long getIdPraia() {
		return idPraia;
	}

	public void setIdPraia(Long idPraia) {
		this.idPraia = idPraia;
	}

	public Long getIdCor() {
		return idCor;
	}

	public void setIdCor(Long idCor) {
		this.idCor = idCor;
	}

	@Override
	public int hashCode() {
		final int prime = 31;
		int result = 1;
		result = prime * result + ((idPraia == null) ? 0 : idPraia.hashCode());
		result = prime * result + ((idCor == null) ? 0 : idCor.hashCode());
		return result;
	}

	@Override
	public boolean equals(Object obj) {
		if (this == obj)
			return true;
		if (obj == null)
			return false;
		if (getClass() != obj.getClass())
			return false;
		PraiaCorPK other = (PraiaCorPK) obj;
		if (idPraia == null) {
			if (other.idPraia != null)
				return false;
		} else if (!idPraia.equals(other.idPraia))
			return false;
		if (idCor == null) {
			if (other.idCor != null)
				return false;
		} else if (!idCor.equals(other.idCor))
			return false;
		return true;
	}

}